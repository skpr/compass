BEGIN
{
    printf("Tracing PHP function calls... Hit Ctrl-C to end.\n");
}

uprobe:FILE:function_begin
{
    printf("FUNCTION BEGIN");
}

uprobe:FILE:function_end
{
    printf("FUNCTION END");
}

uprobe:FILE:request_init
{
    printf("REQUEST BEGIN");
}

uprobe:FILE:request_shutdown
{
    printf("REQUEST END");
}

END
{
    printf("Finished tracing PHP function calls\n");
}
